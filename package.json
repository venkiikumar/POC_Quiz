{
  "name": "java-quiz-application",
  "version": "1.0.0",
  "description": "A web-based Java quiz application with admin panel for managing questions and viewing results",
  "main": "server-minimal.js",
  "scripts": {
    "start": "node server-minimal.js",
    "dev": "nodemon server-backup.js",
    "postinstall": "echo 'Minimal server ready'",
    "client": "npx http-server . -p 3000 -o",
    "serve": "python -m http.server 3000",
    "install-deps": "npm install",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:seed": "node prisma/seed.js",
    "db:reset": "npx prisma migrate reset --force && npm run db:seed"
  },
  "keywords": [
    "quiz",
    "java",
    "education",
    "assessment",
    "web-application"
  ],
  "author": "Quiz App Developer",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "nodemon": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
